<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dyslexia"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.dyslexia.ChildMarkDetails"
            android:label="@string/app_name" >
         </activity>
        <activity
            android:name="com.example.dyslexia.MainActivity"
            android:label="@string/app_name" >
             <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.dyslexia.LoginValidation"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.dyslexia.UserRegister"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.dyslexia.PractiseSession"
            android:label="@string/app_name" >
           
        </activity>
         <activity
            android:name="com.example.dyslexia.TestingSession"
            android:label="@string/app_name" >
           
        </activity>
        <activity
            android:name="com.example.dyslexia.TestResult"
            android:label="@string/app_name" >
           
        </activity>
         <activity
            android:name="com.example.dyslexia.LevelChecker"
            android:label="@string/app_name" >
           
        </activity>
        <activity android:name="org.achartengine.GraphicalActivity" />
    </application>

</manifest>
